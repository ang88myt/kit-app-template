#######################################################################################################################

[settings]
# Fast shutdown for the warmup
app.fastShutdown = true

app.name = "Code"

[settings.app.exts]
folders.'++' = ["${app}/../exts", "${app}/../extscache", "${app}/../apps"]

[settings.app.extensions]
generateVersionLockExclude = ["omni.code"]

